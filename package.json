{
  "name": "grafana-infinity-datasource",
  "version": "1.0.0-dev.5",
  "description": "JSON, CSV, XML, GraphQL & HTML datasource for Grafana. Do infinite things with Grafana.",
  "main": "dist/module.js",
  "scripts": {
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "start": "yarn watch",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "lint": "yarn spellcheck && yarn audit-ci",
    "test": "yarn lint && grafana-toolkit plugin:test",
    "e2e": "yarn lint && grafana-e2e run",
    "e2e:open": "yarn lint && grafana-e2e open",
    "e2e:update": "yarn lint && grafana-e2e run --update-screenshots",
    "build": "yarn lint && grafana-toolkit plugin:build",
    "ci-build": "yarn lint && grafana-toolkit plugin:ci-build",
    "audit-ci": "audit-ci",
    "sign": "grafana-toolkit plugin:sign"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yesoreyeram/grafana-infinity-datasource.git"
  },
  "keywords": [
    "grafana",
    "grafana infinity",
    "grafana utils",
    "inline",
    "random walk",
    "json",
    "simple-json",
    "graphql",
    "csv",
    "html",
    "xml",
    "uql",
    "groq",
    "url",
    "scraping",
    "web scraping",
    "soap",
    "atom",
    "rss",
    "feed"
  ],
  "author": "Sriramajeyam Sugumaran",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/yesoreyeram/grafana-infinity-datasource/issues"
  },
  "homepage": "https://github.com/yesoreyeram/grafana-infinity-datasource#readme",
  "dependencies": {
    "@emotion/css": "^11.9.0",
    "@grafana/data": "8.4.7",
    "@grafana/runtime": "9.0.6",
    "@grafana/ui": "9.0.6",
    "cheerio": "^1.0.0-rc.10",
    "csv-parse": "^4.12.0",
    "groq-js": "0.4.0-beta.2",
    "json-to-pretty-yaml": "^1.2.2",
    "jsonpath-plus": "^6.0.1",
    "lodash": "^4.17.21",
    "mathjs": "^10.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rxjs": "6.6.3",
    "uql": "0.0.17",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@grafana/e2e": "8.5.4",
    "@grafana/e2e-selectors": "8.5.4",
    "@grafana/toolkit": "8.5.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^13.1.9",
    "@types/cheerio": "^0.22.21",
    "@types/lodash": "^4.14.149",
    "@types/mathjs": "^6.0.5",
    "@types/xml2js": "^0.4.6",
    "audit-ci": "^5.1.2",
    "cspell": "^5.19.7",
    "cypress": "7.7.0",
    "jest-runner": "^27.0.4",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "tslib": "^2.3.1",
    "typescript": "4.4.3",
    "vitest": "^0.10.0"
  },
  "resolutions": {
    "rxjs": "6.6.3",
    "minimist": "1.2.6",
    "underscore": "1.12.1",
    "glob-parent": "5.1.2",
    "follow-redirects": "1.14.8",
    "prismjs": "1.27.0",
    "log4js": "6.4.0"
  }
}
