{
  "name": "grafana-infinity-datasource",
  "version": "1.4.0",
  "description": "JSON, CSV, XML, GraphQL, HTML and REST API datasource for Grafana. Do infinite things with Grafana. Transform data with UQL/GROQ. Visualize data from many apis, RSS/ATOM feeds directly",
  "main": "dist/module.js",
  "scripts": {
    "server": "docker-compose up --build",
    "dev": "webpack -w -c ./webpack.config.ts --env development",
    "watch": "webpack -w -c ./webpack.config.ts --env development",
    "start": "yarn watch",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "e2e": "yarn cypress install && yarn grafana-e2e run",
    "e2e:open": "yarn exec cypress open",
    "e2e:update": "yarn cypress install && yarn grafana-e2e run --update-screenshots",
    "build": "webpack -c ./webpack.config.ts --env production",
    "audit-ci": "audit-ci",
    "sign": "npx --yes @grafana/sign-plugin",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yesoreyeram/grafana-infinity-datasource.git"
  },
  "keywords": [
    "grafana",
    "grafana infinity",
    "grafana utils",
    "inline",
    "random walk",
    "json",
    "simple-json",
    "graphql",
    "csv",
    "html",
    "xml",
    "uql",
    "groq",
    "url",
    "scraping",
    "web scraping",
    "soap",
    "atom",
    "rss",
    "feed"
  ],
  "author": "Sriramajeyam Sugumaran",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/yesoreyeram/grafana-infinity-datasource/issues"
  },
  "homepage": "https://yesoreyeram.github.io/grafana-infinity-datasource",
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@grafana/data": "8.4.7",
    "@grafana/runtime": "8.4.7",
    "@grafana/ui": "8.4.7",
    "cheerio": "^1.0.0-rc.10",
